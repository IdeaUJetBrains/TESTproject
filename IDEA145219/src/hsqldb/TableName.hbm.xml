<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hsqldb.TableName" table="TABLE_NAME" schema="PUBLIC" catalog="PUBLIC">
        <!--<id name="id"/>-->
        <property name="column1">
            <column name="COLUMN_1" sql-type="integer(32)" not-null="true"/>
        </property>
        <property name="column2">
            <column name="COLUMN_2" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>