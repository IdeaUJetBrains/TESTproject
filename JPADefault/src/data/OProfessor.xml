<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="data.OProfessor">
        <table name="PROFESSOR" schema="PUBLIC" catalog="PUBLIC"/>
        <attributes>
            <id name="id">
                <column name="ID" precision="32"/>
            </id>
            <basic name="name">
                <column name="NAME" column-definition="UNKNOWN" nullable="false"/>
            </basic>
            <basic name="salary">
                <column name="SALARY" nullable="false" precision="16777216"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
