<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="entities.City" table="IDEA145144_CITY" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer(32)"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(255)"/>
        </property>
        <property name="state">
            <column name="STATE" sql-type="boolean(0)"/>
        </property>
        <property name="country">
            <column name="COUNTRY" sql-type="varchar(255)"/>
        </property>
        <property name="map">
            <column name="MAP" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>