<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="data.data1.Professor" table="PROFESSOR" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer(32)"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="salary">
            <column name="SALARY" sql-type="varbinary(16777216)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>